(select u.name as results 
from users u join movierating m 
group by u.name
order by count(*) desc,name
limit 1)

union all

(select m.title as results 
from Movies  m join movierating o
using(movie_id)
where year(o.created_at)=2020 and month(o.created_at) = 2
group by m.title
order by avg(o.rating) desc,title
limit 1)
